função fFib( pTermo ) {
	vA = 1 .
	vB = 1 .
	vI = 0 .
	enquanto vI < pTermo repetir incrementando vI {
		vT = vB .
		vB = vA + vB .
		vA = vT .
	}
	retornar vA .
}

função fFibR( pTermo ) {
	se pTermo é igual a 0 OU pTermo é igual a 1 então {
		vR = 1 .
	} senão {
		vR = fFibR( pTermo - 2 ) + fFibR( pTermo - 1 ) .
	}
}

fFib( 1 ) .